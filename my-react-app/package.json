{
  "name": "my-react-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/*.tsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/env"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.16",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "axios": "^1.3.4",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "query": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "npx eslint src",
    "test": "vitest watch",
    "test:coverage": "npm test -- --coverage",
    "test:coverage1": "vitest run -- --coverage",
    "dev": "ts-node server.tsx",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "cypress": "cypress open",
    "cypress:report": "npx nyc report && npx nyc report --reporter=text-summary",
    "cypress:run": "cypress run --e2e",
    "cypress:e2e": "npx start-server-and-test dev http-get://localhost:3001 cypress:run",
    "cypress:coverage": "npm run cypress:e2e && npm run cypress:report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/coverage-istanbul": "^0.30.1",
    "babel-jest": "^29.5.0",
    "babel-plugin-istanbul": "^6.1.1",
    "c8": "^7.13.0",
    "cypress": "^12.11.0",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^27.5.1",
    "jsdom": "^21.1.1",
    "nyc": "^15.1.0",
    "prettier": "2.8.4",
    "react-hook-form": "^7.43.9",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^2.0.0",
    "vite": "^4.3.2",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  }
}
